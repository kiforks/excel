{
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"extends": [
		"eslint:recommended",
		"google"
	],
	"parser": "babel-eslint",
	"rules": {
		"indent": ["error", 4],
		"no-tabs": [
			"error",
			{
				"allowIndentationTabs": true
			}
		],
		"object-curly-spacing": ["error", "always"],
		"arrow-parens": "off",
		"comma-dangle": "off",
		"require-jsdoc": "off",
		"no-mixed-spaces-and-tabs": ["error", "smart-tabs"]
	}
}
